<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:ignore="ExtraText">

    <LinearLayout
        android:id="@+id/survey_layout_medtranspubli"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        style="@style/SurveySingleQuestion">

        <TextView
            android:id="@+id/survey_text_medtranspubli"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/survey_text_medtranspubli"
            style="@style/SurveyQuestionTitle"/>

        <TextView
            android:id="@+id/survey_text_medtranspubli_ent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/survey_text_medtranspubli_ent"
            android:visibility="visible"
            style="@style/SurveyQuestionHelp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="600dp"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <CheckBox
                    android:id="@+id/check_medtranspubli_option7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/survey_check_medtranspubli_option7" />

                <CheckBox
                    android:id="@+id/check_medtranspubli_option8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/survey_check_medtranspubli_option8" />

                <CheckBox
                    android:id="@+id/check_medtranspubli_option10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/survey_check_medtranspubli_option10" />

                <CheckBox
                    android:id="@+id/check_medtranspubli_option11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/survey_check_medtranspubli_option11" />

                <CheckBox
                    android:id="@+id/check_medtranspubli_option99"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/survey_check_medtranspubli_option99" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/survey_text_medtranspubli_1"
                    android:layout_width="match_parent"
                    android:layout_height="48dp" />

                <TextView
                    android:id="@+id/survey_text_medtranspubli_2"
                    android:layout_width="match_parent"
                    android:layout_height="48dp" />

                <TextView
                    android:id="@+id/survey_text_medtranspubli_3"
                    android:layout_width="match_parent"
                    android:layout_height="48dp" />

                <TextView
                    android:id="@+id/survey_text_medtranspubli_4"
                    android:layout_width="match_parent"
                    android:layout_height="48dp" />

                <EditText
                    android:id="@+id/survey_edit_medtranspubli"
                    android:layout_width="match_parent"
                    android:layout_height="44dp"
                    android:gravity="center"
                    android:inputType="text"
                    style="@style/SurveyQuestionEdit"
                    android:visibility="visible" />

            </LinearLayout>
        </LinearLayout>

        <EditText
            android:id="@+id/survey_edit_medtranspubli1"
            style="@style/SurveyQuestionEdit"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:visibility="gone"/>

        <EditText
            android:id="@+id/survey_edit_medtranspubli2"
            style="@style/SurveyQuestionEdit"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:visibility="gone"/>

        <EditText
            android:id="@+id/survey_edit_medtranspubli3"
            style="@style/SurveyQuestionEdit"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:visibility="gone"/>
    </LinearLayout>
</LinearLayout>