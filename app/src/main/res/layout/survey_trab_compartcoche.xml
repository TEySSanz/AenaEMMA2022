<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:ignore="ExtraText">

    <LinearLayout
        android:id="@+id/survey_layout_compartcoche"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible"
        style="@style/SurveySingleQuestion">

        <TextView
            android:id="@+id/survey_text_compartcoche"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/survey_text_compartcoche"
            style="@style/SurveyQuestionTitle"/>

        <TextView
            android:id="@+id/survey_text_compartcoche_ent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/survey_text_compartcoche_ent"
            android:visibility="visible"
            style="@style/SurveyQuestionHelp"/>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="600dp"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <CheckBox
                    android:id="@+id/check_compartcoche_option0"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/survey_check_compartcoche_option0" />

                <CheckBox
                    android:id="@+id/check_compartcoche_option1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/survey_check_compartcoche_option1" />

                <CheckBox
                    android:id="@+id/check_compartcoche_option2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/survey_check_compartcoche_option2" />

                <CheckBox
                    android:id="@+id/check_compartcoche_option3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/survey_check_compartcoche_option3" />

                <CheckBox
                    android:id="@+id/check_compartcoche_option4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/survey_check_compartcoche_option4" />

                <CheckBox
                    android:id="@+id/check_compartcoche_option5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/survey_check_compartcoche_option5" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/survey_text_compartcoche_1"
                    android:layout_width="match_parent"
                    android:layout_height="48dp" />

                <TextView
                    android:id="@+id/survey_text_compartcoche_2"
                    android:layout_width="match_parent"
                    android:layout_height="48dp" />

                <TextView
                    android:id="@+id/survey_text_compartcoche_3"
                    android:layout_width="match_parent"
                    android:layout_height="48dp" />

                <TextView
                    android:id="@+id/survey_text_compartcoche_4"
                    android:layout_width="match_parent"
                    android:layout_height="48dp" />

                <TextView
                    android:id="@+id/survey_text_compartcoche_5"
                    android:layout_width="match_parent"
                    android:layout_height="51dp" />

                <EditText
                    android:id="@+id/survey_edit_compartcoche"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:gravity="center"
                    android:inputType="text"
                    style="@style/SurveyQuestionEdit"
                    android:visibility="visible" />

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>